<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.login.Coordinator.FragmentCoordinatorAnalysisRequest">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="403dp"
        android:layout_height="54dp"
        android:layout_marginTop="20dp"
        android:background="#AFC4E4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCAR_Analysis" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pendientes"
        android:textSize="22dp"
        app:layout_constraintTop_toTopOf="@id/imageView4"
        app:layout_constraintStart_toStartOf="@id/imageView4"
        app:layout_constraintEnd_toEndOf="@id/imageView4"
        app:layout_constraintBottom_toBottomOf="@id/imageView4"
        android:textColor="#FFFFFFFF" />

    <TextView
        android:id="@+id/txtCAR_Analysis"
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="55dp"
        android:text="ANÁLISIS ACADÉMICO"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollViewCAR"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView4">

        <LinearLayout
            android:id="@+id/linearLayoutCAR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"></LinearLayout>
    </ScrollView>


</android.support.constraint.ConstraintLayout>